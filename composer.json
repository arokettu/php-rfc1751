{
    "name": "arokettu/rfc1751",
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "Arokettu\\Rfc1751\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arokettu\\Rfc1751\\Tests": "tests"
        }
    },
    "require": {
        "php": "^7.1 | ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 | ^8.5 | ^9.5",
        "sandfox.dev/code-standard": "^1.2023.12.09",
        "squizlabs/php_codesniffer": "*",
        "vimeo/psalm": "^5.18"
    }
}
